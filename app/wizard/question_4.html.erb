<%= stylesheet_link_tag "screens/wizard" %>

<div class="uk-grid uk-grid-large uk-flex-center">
	<div class="uk-panel uk-panel-box">
		<p class="uk-text-right"><%= _("#{t('wizard.q4_welcome')}") %></p></br>
		<div>
			<table>
				<tr>
					<th class="uk-width-1-4"></th>
					<th class="uk-width-1-4"><%= link_to @A41, next_wizard_path(:q => '1', :qna_id => @qna_id), method: :put %> </th>
					<th class="uk-width-1-4"></th>
					<th class="uk-width-1-4"></th>
				</tr>
				<tr>
					<th class="uk-width-1-4"><%= _("#{t('wizard.Im')}") %></th>
					<th class="uk-width-1-4"></th>
					<th class="uk-width-1-4"><%= _("#{t('wizard.my')}") %></th>
					<th class="uk-width-1-4"><%= image_tag(@image_url) %></th>
				</tr>
				<tr>
					<th class="uk-width-1-4"></th>
					<th class="uk-width-1-4"><%= link_to @A42, next_wizard_path(:q => '2', :qna_id => @qna_id), method: :put %> </th>
					<th class="uk-width-1-4"></th>
					<th class="uk-width-1-4"></th>
				</tr>
			</table>
		</div>
		<div class="uk-text-right">
			<%= link_to _("#{t('common.back')}"), previous_wizard_path(:qna_id => @qna_id) %> | 
			<%= link_to _("#{t('common.skip')}"), wizard_path(:project_details) %>
		</div>
	</div>
</div>
